+        pmsProductCategoryLambdaQueryWrapper.eq(PmsProductCategory::getId,productCategoryId);
+        PmsProductCategory pmsProductCategory = pmsProductCategoryService.getOne(pmsProductCategoryLambdaQueryWrapper);
+        productUpdateInitDTO.setCateParentId(pmsProductCategory.getParentId());
+        return  create(productUpdateInitDTO);
+    }
+
+    private void saveManyList(List list, Long productId, IService service){
+        if (list.isEmpty()) return;
+        for (Object obj : list) {
+            try {
+                Method setProductId = obj.getClass().getMethod("setProductId", Long.class);
+                setProductId.invoke(obj, productId);
+                Method setId = obj.getClass().getMethod("setId", Long.class);
+                setId.invoke(obj, (Long) null);
+            } catch (Exception e) {
+                e.printStackTrace();
+            }
+        }
+        service.saveOrUpdateBatch(list);
+    }
 }
